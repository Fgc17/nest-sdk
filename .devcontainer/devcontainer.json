{
    "name": "nest-sdk",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "initializeCommand": "ls",
    "forwardPorts": [1212, 1213, 1214, 3000, 4000, 5000, 6000, 7000, 8080],
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-azuretools.vscode-docker",
          "dbaeumer.vscode-eslint",
          "esbenp.prettier-vscode@9.14.0",
          "PKief.material-icon-theme",
          "natqe.reload",
          "maxcutlyp.dotenv-autocomplete",
          "GitHub.copilot",
          "rangav.vscode-thunder-client",
          "eamodio.gitlens",
          "yoavbls.pretty-ts-errors",
          "IronGeek.vscode-env",
          "ms-vscode.vscode-typescript-next"
        ],
        "settings": {
          "extensions.autoCheckUpdates": false,
          "extensions.autoUpdate": false,
          "editor.defaultFormatter": "esbenp.prettier-vscode",
          "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
          "[typescriptreact]": {
            "editor.defaultFormatter": "esbenp.prettier-vscode"
          },
          "[prisma]": {
            "editor.defaultFormatter": "Prisma.prisma-insider"
          },
          "editor.codeActionsOnSave": {
            "source.fixAll.eslint": true
          },
          "editor.formatOnSave": true,
          "editor.formatOnPaste": false,
          "prettier.useEditorConfig": false,
          "prettier.configPath": "./.prettierrc.js",
          "prettier.prettierPath": "./node_modules/prettier",
          "typescript.tsdk": "node_modules/typescript/lib",
          "eslint.workingDirectories": ["./"],
          "material-icon-theme.activeIconPack": "nest",
          "explorer.compactFolders": false,
          "typescript.preferences.importModuleSpecifier": "relative",
          "workbench.tree.indent": 20,
          "material-icon-theme.folders.associations": {
            "filters": "Rules",
            "entities": "Class",
            "dto": "Shared",
            "strategies": "Fastlane"
          },
          "material-icon-theme.files.associations": {
            "*.socket.ts": "Silverstripe"
          }
        }
      }
    }
  }
  